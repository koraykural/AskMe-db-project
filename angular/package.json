{
  "name": "angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"ng serve --host 0.0.0.0\" \"ngtw watch\"",
    "build": "ngtw build --purge && ng build --prod --aot=true --output-hashing=all",
    "build:stats": "ngtw build --purge && ng build --prod --aot=true --output-hashing=all --stats-json",
    "analyze": "webpack-bundle-analyzer ../app/public/stats.json",
    "postbuild": "gulp purifyCSS && gulp compress && npm run css",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "css": "ngtw build",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.6",
    "@angular/common": "~9.0.6",
    "@angular/compiler": "~9.0.6",
    "@angular/core": "~9.0.6",
    "@angular/forms": "~9.0.6",
    "@angular/platform-browser": "~9.0.6",
    "@angular/platform-browser-dynamic": "~9.0.6",
    "@angular/router": "~9.0.6",
    "@auth0/angular-jwt": "^5.0.1",
    "@fortawesome/angular-fontawesome": "^0.6.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "hamburgers": "^1.1.3",
    "ngx-textarea-autosize": "^2.0.3",
    "ngx-timeago": "^2.0.0",
    "rxjs": "~6.5.4",
    "semantic-ui-css": "^2.4.1",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1001.7",
    "@angular/cli": "~9.0.6",
    "@angular/compiler-cli": "~9.0.6",
    "@angular/language-service": "~9.0.6",
    "@tailwindcss/custom-forms": "^0.2.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "concurrently": "^5.3.0",
    "gulp": "^4.0.2",
    "gulp-gzip": "^1.4.2",
    "gulp-purifycss": "^0.2.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "prettier": "^2.1.2",
    "protractor": "~5.4.3",
    "tailwindcss": "^1.9.5",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.5",
    "webpack-bundle-analyzer": "^4.2.0"
  }
}
